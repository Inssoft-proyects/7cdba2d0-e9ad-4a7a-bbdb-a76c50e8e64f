                    <span>
                            @(Html.Kendo().DropDownListFor(t => t.IdTipoMotivoIngreso).Filter(FilterType.Contains)
                            .DataTextField("Text")
                            .DataValueField("Value")
                            .OptionLabel("Selecciona una opción")
                            .DataSource(source =>
                            {
