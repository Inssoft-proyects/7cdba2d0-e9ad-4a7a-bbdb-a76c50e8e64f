            <div class="col-12 col-md-6 col-lg-5 col-xl-auto">
                <div class="row align-items-center justify-content-center justify-content-md-start">
                    <div class="col-auto">
                        <div class="border border-2 border-dark rounded-circle">
                            <h5 class="m-0 numberCard d-flex align-items-center justify-content-center">
                                <b>1</b>
                            </h5>
                        </div>
                    </div>
                    <div class="col-auto">
                        <i class="icon-factorAccidente h1 colorPrimary"></i>
                    </div>
                    <div class="col-auto my-3">
                        <h2 class="m-0 h3"><b>Factores que intervienen en el accidente</b></h2>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-8 pe-0">
                <div class="bgSuccess py-1"></div>
            </div>
            <div class="col-4 ps-0">
                <div class="bg-light py-1"></div>
            </div>
        </div>
        <div class="row justify-content-between align-items-end mb-1">
            <div class="col-12 col-md-auto mt-4 mb-2">
                <h5 class="px-4"><b>Listado de factores del accidente</b></h5>
                <h6 class="px-4 text-muted">Da de alta los factores del accidente.</h6>
            </div>
            <div class="col-12 col-lg-auto text-center">
                <div class="btnOutline btnOutlinePrimary px-4 my-3">
                    <button data-bs-toggle="modal" data-bs-target="#addFactor">
                        <h5 class="m-0"><i class="me-2 icon-addFactorAccidente"></i><b>Agregar factor</b></h5>
                    </button>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-12 my-4 px-4 rounded">
                <kendo-grid name="Factores" height="450">
                    <datasource type="DataSourceTagHelperType.Custom" custom-type="odata" page-size="20">
                        <transport>
                            <read url="https://demos.telerik.com/kendo-ui/service/Northwind.svc/Orders" />
                        </transport>

                    </datasource>
                    <sortable enabled="true" />

                    <pageable button-count="5" refresh="true" page-sizes="new int[] { 5, 10, 20 }">
                    </pageable>
                    <columns>
                        <column field="ShipName" title="Factor" width="120" />
                        <column field="ShipName" title="Opción" width="120" />
                        <column template="<button data-bs-toggle=modal data-bs-target=\#editFactor class='w-100 btn'><h6 class='m-0 colorPrimary'><i class='h5 icon-edit me-2'></i><b>Editar</b></h6></button>" width="150" />
                        <column template="<button data-bs-toggle=modal data-bs-target=\#deleteFactor class='w-100 btn'><h6 class='m-0 colorDanger'><i class='h5 icon-delete me-2'></i><b>Eliminar</b></h6></button>" width="150" />
                    </columns>

                </kendo-grid>
            </div>
        </div>

    </section>

    <section class="mx-md-3 mx-lg-4 mx-xl-5 bg-white boxShadow my-5 rounded">
