         $('#confirmacionModal').modal('hide');
          window.close(); // Cierra la ventana
        });
    }
    function cerrarModalFisica(){
   $('#addFisico').modal('hide');
 
}
function cerrarModalMoral(){
   $('#addMoral').modal('hide');

